{
  "name": "secmaze",
  "version": "1.0.2",
  "description": "Blockchain-powered defense maze system protecting digital assets from advanced attacks",
  "main": "src/backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "nodemon src/backend/server.js",
    "build": "npm run build:frontend",
    "build:frontend": "cd src/frontend && npm run build",
    "start": "node src/backend/server.js",
    "test": "jest",
    "lint": "eslint ."
  },
  "keywords": [
    "security",
    "blockchain",
    "web3",
    "bot-protection",
    "defense",
    "challenge",
    "maze",
    "machine-learning"
  ],
  "author": "SecMaze-AI",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.1",
    "web3": "^4.0.3",
    "ethers": "^6.7.1",
    "bcrypt": "^5.1.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SecMaze-AI/SecMaze.git"
  },
  "bugs": {
